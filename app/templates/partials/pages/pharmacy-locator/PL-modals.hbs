<!-- Modal -->
<div class="modal fade" id="countyModal" tabindex="-1" role="dialog" aria-labelledby="countyModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" data-dismiss="modal" aria-label="Close" class="close">
          {{> close}}
        </button>
        <h1 class="modal-title h3 margin-none" id="countyModalLabel">Select a County</h1>
      </div>
      <div class="modal-body" style="padding-bottom: 30px;">
        <p>Please confirm the county of your residence</p>
        <a href="#" data-dismiss="modal" aria-label="Close">County One</a>
        <br>
        <a href="#" class="border-none" data-dismiss="modal" aria-label="Close">County Two</a>
      </div>
    </div>
  </div>
</div>
