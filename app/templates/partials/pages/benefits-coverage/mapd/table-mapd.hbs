<!-- Mobile table start -->
<div class="mobile-table-wrapper">
  <div class="row">
    <div class="col-md-12 clearfix">
      <!-- Slick Navigation -->
      <div class="table-nav-block">
        <div class="prev-arrow">
          <span class="material-icons">keyboard_arrow_left</span> Prev
        </div>
        <div class="nav-dots"></div>
        <div class="next-arrow">
          Next
          <span class="material-icons">keyboard_arrow_right</span>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <div class="table-slick">
        <div class="table-slick-slide">
          <!-- Table column one -->
          <table class="mobile-table">
            <tr>
              <th scope="col" colspan="2" class="align-center" id="ads-header-mobile">
                <span>Annual Deductible Stage</span>
              </th>
            </tr>
            <tr>
              <th scope="row">Covered Generic Drugs</th>
              <td rowspan="2" headers="ads-header-mobile">No Deductible</td>
            </tr>
            <tr>
              <th scope="row">All Other Covered Drugs</th>
            </tr>
          </table>
        </div>
        <div class="table-slick-slide">
          <!-- Table column two -->
          <table class="mobile-table">
            <tr>
              <th scope="col" colspan="2" class="align-center" id="ics-header-mobile">
                <span>Initial Coverage Stage</span>
              </th>
            </tr>
            <tr>
              <th scope="row">Covered Generic Drugs</th>
              <td headers="ice-header-mobile">$X.XX</td>
            </tr>
            <tr>
              <th scope="row">All Other Covered Drugs</th>
              <td headers="ice-header-mobile">X.XX</td>
            </tr>
          </table>
        </div>
        <div class="table-slick-slide">
          <!-- Table column three -->
          <table class="mobile-table">
            <tr>
              <th scope="col" colspan="2" class="align-center" id="ccs-header-mobile">
                <span>Catastrophic Coverage Stage</span>
              </th>
            </tr>
            <tr>
              <th scope="row">Covered Generic Drugs</th>
              <td headers="ccs-header-mobile">$X.XX</td>
            </tr>
            <tr>
              <th scope="row">All Other Covered Drugs</th>
              <td headers="ccs-header-mobile">X.XX</td>
            </tr>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- Desktop table start -->
<div class="desktop-table-wrapper">
